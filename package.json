{
  "name": "wbr",
  "version": "2.0.0",
  "description": "webpack / babel / react - boilerplate",
  "main": "index.js",
  "scripts": {
    "dev": "webpack-dev-server --inline --hot --mode development",
    "build": "webpack --config webpack.config.js --mode production",
    "ndev": "NODE_ENV=development webpack-dev-server --inline --hot --mode development",
    "nbuild": "NODE_ENV=production webpack --config webpack.config.js --mode production",
    "wndev": "SET NODE_ENV=development  && webpack-dev-server --inline --hot --mode development",
    "wnbuild": "SET NODE_ENV=production  && webpack --config webpack.config.js --mode production"
  },
  "author": "Pomatsuev Stanislav",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.4.4",
    "@babel/plugin-proposal-class-properties": "^7.4.4",
    "@babel/plugin-proposal-decorators": "^7.4.4",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/polyfill": "^7.4.4",
    "@babel/preset-env": "^7.4.4",
    "@babel/preset-react": "^7.0.0",
    "autoprefixer": "^9.5.1",
    "babel-loader": "^8.0.6",
    "css-loader": "^2.1.1",
    "file-loader": "^4.0.0",
    "html-webpack-plugin": "^3.2.0",
    "mini-css-extract-plugin": "^0.6.0",
    "node-sass": "^4.12.0",
    "postcss-loader": "^3.0.0",
    "react-hot-loader": "^4.8.7",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.1",
    "webpack": "^4.31.0",
    "webpack-cli": "^3.3.2",
    "webpack-dev-server": "^3.4.1"
  },
  "dependencies": {
    "react": "^16.8.6",
    "react-dom": "^16.8.6"
  }
